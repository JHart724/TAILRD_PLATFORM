{
  "name": "tailrd-backend",
  "version": "1.0.0",
  "description": "TAILRD Platform Backend - EHR Integration and Analytics API",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:setup": "prisma generate && prisma migrate dev && npm run seed",
    "db:migrate": "psql $DATABASE_URL -f scripts/setup-database-schema.sql",
    "setup:redox": "./scripts/setup-redox.sh",
    "setup:cloudticity": "./scripts/setup-cloudticity.sh",
    "deploy:staging": "npm run build && npm run deploy:staging:run",
    "deploy:staging:run": "NODE_ENV=staging npm start",
    "deploy:production": "npm run build && npm run deploy:production:run",
    "deploy:production:run": "NODE_ENV=production npm start",
    "monitor:memory": "node --expose-gc scripts/memory-monitor.js",
    "verify:redox": "node scripts/verify-redox.js",
    "verify:database": "node scripts/verify-database.js",
    "migrate:prod": "node scripts/migrate-production.js",
    "backup:database": "pg_dump $DATABASE_URL > backups/db-$(date +%Y%m%d_%H%M%S).sql",
    "logs:tail": "tail -f logs/combined.log",
    "logs:errors": "tail -f logs/error.log",
    "logs:audit": "tail -f logs/audit.log",
    "health:check": "curl -f http://localhost:$PORT/api/health || exit 1",
    "security:scan": "npm audit && snyk test",
    "docker:build": "docker build -t tailrd-backend .",
    "docker:run": "docker run -p 3001:3001 --env-file .env tailrd-backend",
    "precommit": "npm run lint && npm run test"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "healthcare",
    "ehr",
    "fhir",
    "redox",
    "cardiology",
    "analytics"
  ],
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "@prisma/client": "^5.6.0",
    "redis": "^4.6.10",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "crypto": "^1.0.1",
    "axios": "^1.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.0",
    "typescript": "^5.2.2",
    "prisma": "^5.6.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}